@using TerminalSite.Shared.TerminalResponses;

@foreach (ITerminalResponse response in responses)
{
    @response.Render();
}

@code {
    public List<ITerminalResponse> responses = new List<ITerminalResponse>();

    public void AddResponse(ITerminalResponse response)
    {
        responses.Add(response);
        StateHasChanged();
    }
}
